{
	"compilerOptions": {
		"incremental": true,
		"target": "ESNext",
		"module": "commonjs",
		"declaration": false,
		"outDir": "dist",
		"sourceMap": false,
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
